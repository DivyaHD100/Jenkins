pipeline {
    agent any
    stages{
        stage ('stage -1'){
            steps {
                sh "echo 1st stage"
            }


        }
         stage ('stage -2'){
            steps {
                sh "echo 2nd stage"
            }


        }
         stage ('stage -3'){
            steps {
                sh "echo 3rd stage"
            }


        }

    }
}
 